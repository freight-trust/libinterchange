        "organizer",
        "programme",
        "remainingAttendeeCapacity",
        "subEvent",
        "schedulingNote",
        "superEvent",
        "url"
      ]
    },
    "response": {
      "requiredFields": [
        "type",
        "id",
        "startDate",
        "facilityUse"
      ],
      "recommendedFields": [
        "endDate",
        "duration",
        "maximumUses",
        "remainingUses"
      ],
      "shallNotInclude": [
        "offers"
      ]
    }
  },
  "inSpec": [
